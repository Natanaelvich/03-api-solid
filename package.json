    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.7.1",
    "@prisma/client": "^4.11.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-rate-limiter": "^0.1.3",
    "supertest": "^6.3.3",
    "zod": "^3.21.4"
  }
